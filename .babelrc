{
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": "current"
          }
        }
      ]
    ],
    "plugins": [
      ["module-resolver", {
        "root": ["./src"],
        "alias": {
          "@core": "./src/core",
          "@components": "./src/components",
        }
      }]
    ]
  }